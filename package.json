  },
  "homepage": "https://github.com/telemark/search-service#readme",
  "now": {
    "alias": [
      "search.service.t-fk.no"
    ],
    "type": "npm",
    "dotenv": "production.env",
    "engines": {
      "node": "10.1.0"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
